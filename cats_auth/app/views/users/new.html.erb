<h1>Sign Up!</h1>
<form action="<%= users_url(@user) %>" method = "POST">
  <label>Username
  <input type="text" name = "user[user_name]" value ="<%= @user.user_name %>">
  </label>
  <br>
   <label>Password
  <input type="text" name = "user[password]" value ="<%= @user.password %>">
  </label>
  <input type="submit" value ="Sign up">
</form>

